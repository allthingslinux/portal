{
  "name": "portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "deduplicate": "pnpm dedupe",
    "dev": "next dev",
    "dev:grab": "pnpm dlx @react-grab/cursor@latest && next dev",
    "dev:turbo": "next dev --turbopack",
    "dev:https": "next dev --experimental-https",
    "scan": "NEXT_PUBLIC_REACT_SCAN_ENABLED=true next dev & pnpm dlx react-scan@latest localhost:3000",
    "build": "next typegen && next build",
    "build:debug": "next build --debug",
    "build:profile": "next build --profile",
    "build:prerender-debug": "next build --debug-prerender",
    "start": "next start",
    "typegen": "next typegen",
    "type-check": "tsc --noEmit",
    "check": "ultracite check",
    "fix": "ultracite fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "info": "next info",
    "telemetry:enable": "next telemetry enable",
    "telemetry:disable": "next telemetry disable",
    "upgrade": "next upgrade",
    "analyze": "next experimental-analyze",
    "analyze:output": "next experimental-analyze --output",
    "auth:init-schema": "pnpm dlx @better-auth/cli@latest generate --output src/shared/db/auth-schema.ts",
    "db:generate": "drizzle-kit generate --config src/shared/db/config.ts",
    "db:migrate": "drizzle-kit migrate --config src/shared/db/config.ts",
    "db:push": "drizzle-kit push --config src/shared/db/config.ts",
    "db:studio": "drizzle-kit studio --config src/shared/db/config.ts",
    "db:seed": "tsx scripts/seed.ts",
    "db:seed:reset": "tsx scripts/seed.ts reset",
    "create-admin": "tsx scripts/create-admin.ts",
    "compose:db": "docker compose up -d portal-db",
    "compose:db:down": "docker compose down",
    "compose:production": "docker compose --profile production up -d",
    "compose:production:down": "docker compose --profile production down",
    "compose:staging": "docker compose --profile staging up -d",
    "compose:staging:down": "docker compose --profile staging down",
    "compose:adminer": "docker compose --profile adminer up -d",
    "compose:adminer:down": "docker compose --profile adminer down",
    "release": "semantic-release"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@better-auth/oauth-provider": "^1.4.13",
    "@better-auth/passkey": "^1.4.13",
